{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": ["DOM", "ES6"],
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "strict": true,
        "target": "ES6"
    }
}
